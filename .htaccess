RewriteEngine On
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^www\. [NC]
RewriteRule ^ https://kollk.com%{REQUEST_URI} [L,NE,R=301]


RewriteEngine on
RewriteCond %{HTTP_REFERER} !^$
RewriteCond %{HTTP_REFERER} !^http(s)?://(www.)?kollk.com [NC]
RewriteRule .(jpg|jpeg|png|gif)$ â€“ [NC,F,L]

## EXPIRES CACHING ##
<IfModule mod_expires.c>
	ExpiresActive On
	ExpiresByType image/jpg "access 1 month"
	ExpiresByType image/jpeg "access 1 month"
	ExpiresByType image/gif "access 1 month"
	ExpiresByType image/png "access 1 month"
	ExpiresByType text/css "modification 1 day"
	ExpiresByType text/html "modification 1 day"
	ExpiresByType text/php "modification 2 hours"
	ExpiresByType application/pdf "access 1 month"
	ExpiresByType text/x-javascript "access 1 month"
	ExpiresByType application/x-shockwave-flash "access 1 month"
	ExpiresByType image/x-icon "access 1 month"
	ExpiresDefault "access 1 day"
</IfModule>
<IfModule mod_headers.c>
    Header set Cache-Control "private max-age=5"
</IfModule>

